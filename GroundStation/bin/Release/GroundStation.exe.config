    <!-- "C:\Users\cubeth\Desktop\RemoteTransceiverBase\hamlib\hamlib-w32-3.1\bin\\rotctld.exe"  -->
    <!-- Hamlib Transceiver selection -->
    <!-- TRX1 is a simulated transceiver -->
    <add key="HamlibInvocationParametersTRX1" value="-m 1 -t 4534" />
